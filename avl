ifndef AVL_H
#define AVL_H

#include <stdint.h>

typedef struct AVL {
    uint32_t key;            
    void *data;              
    struct AVL *left;
    struct AVL *right;
    int8_t height;
} AVL;

// Fonctions AVL
AVL *avl_insert(AVL *root, uint32_t key, void *data, int *err);
AVL *avl_search(AVL *root, uint32_t key);
void avl_free(AVL *root);
#endif
